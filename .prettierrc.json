{
  "arrowParens": "avoid",
  "importOrder": [
    "^(next(.*)|react|react-dom)$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderCaseInsensitive": true,
  "importOrderParserPlugins": ["jsx", "typescript"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "semi": false
}
